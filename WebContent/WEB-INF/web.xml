<?xml version="1.0" encoding="UTF-8"?>

 <web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
  <!-- Search -->       
         <servlet>
        <servlet-name>booking</servlet-name>
        <servlet-class>online.E.booking.BookigServelet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>booking</servlet-name>
        <url-pattern>/book</url-pattern>
    </servlet-mapping>
   
   
   
  <!-- Insert -->    
     <servlet>
        <servlet-name>bookinginsert</servlet-name>
        <servlet-class>online.E.booking.BookingInsert</servlet-class>
    </servlet>
     <servlet-mapping>
        <servlet-name>bookinginsert</servlet-name>
        <url-pattern>/insert</url-pattern>
    </servlet-mapping>
    
    
    
   
   
 <!-- Update -->    
   <servlet>
        <servlet-name>updatebooking</servlet-name>
        <servlet-class>online.E.booking.UpdateBookingServelet</servlet-class>
    </servlet>
     <servlet-mapping>
        <servlet-name>updatebooking</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping> 
    
    
 <!-- Delete -->     
      <servlet>
        <servlet-name>deletebooking</servlet-name>
        <servlet-class>online.E.booking.DeleteBookingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>deletebooking</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
   
    
    
    
</web-app>